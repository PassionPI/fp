{
  "name": "fp-async",
  "version": "0.1.3",
  "description": "",
  "scripts": {
    "build": "npm run build:check && npm run build:bundle && npm run build:type",
    "build:bundle": "vite build",
    "build:check": "npm run test:run && tsc --noEmit",
    "build:type": "tsc -p ./types.json -emitDeclarationOnly && rollup -c rollup.config.ts",
    "debug:file": "esno",
    "test:dev": "vitest",
    "test:run": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PassionPI/fp-async.git"
  },
  "author": "PassionPI <472121553@qq.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/PassionPI/fp-async/issues"
  },
  "homepage": "https://github.com/PassionPI/fp-async#readme",
  "devDependencies": {
    "c8": "^7.11.2",
    "esno": "^0.14.1",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.10.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/fp-async.cjs.js",
  "module": "./dist/fp-async.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fp-async.es.js",
      "require": "./dist/fp-async.cjs.js",
      "default": "./dist/fp-async.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "async",
    "fp",
    "functional",
    "utility"
  ]
}
